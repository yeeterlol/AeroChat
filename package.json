		"@electron-toolkit/utils": "^2.0.0",
		"@electron/remote": "^2.1.0",
		"@hcaptcha/react-hcaptcha": "^1.9.2",
		"@lottiefiles/react-lottie-player": "^3.5.3",
		"@sentry/react": "^7.88.0",
		"@types/react-scroll-to-bottom": "^4.2.4",
		"@types/uuid": "^9.0.7",
		"bbcode-parser": "^1.0.10",
		"bufferutil": "^4.0.8",
		"custom-electron-prompt": "^1.5.7",
		"deepmerge": "^4.3.1",
		"deepmerge-ts": "^5.1.0",
		"discord-api-types": "^0.37.63",
		"discord-protos": "^1.0.5",
		"discord.js-selfbot-v13": "^2.15.0",
		"electron-store": "^8.1.0",
		"electron-updater": "^6.1.1",
		"emojione": "^4.5.0",
		"font-color-contrast": "^11.1.0",
		"fuse.js": "^7.0.0",
		"has-emoji": "^3.0.0",
		"json-bigint-patch": "^0.0.8",
		"jsonwebtoken": "^9.0.2",
		"libsodium-wrappers": "^0.7.13",
		"node-gyp": "^10.0.1",
		"node-microphone": "^0.1.6",
		"node-vibrant": "^3.1.6",
		"public-ip": "^6.0.1",
		"raji": "^0.1.6",
		"react-element-to-jsx-string": "^15.0.0",
		"react-markdown": "^9.0.1",
		"react-router-dom": "^6.18.0",
		"react-scroll-to-bottom": "^4.2.0",
		"react-use-websocket": "^4.5.0",
		"relative-time-format": "^1.1.6",
		"remark-gfm": "^4.0.0",
		"sanitize-html": "^2.11.0",
		"semver": "^7.5.4",
		"speaker": "^0.5.4",
		"to-ico": "^1.1.5",
		"uiohook-napi": "^1.5.2",
		"universal-emoji-parser": "^1.0.122",
		"utf-8-validate": "^5.0.10",
		"uuid": "^9.0.1",
		"ws": "^8.14.2",
		"ya-bbcode": "^4.0.0",
